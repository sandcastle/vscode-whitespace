{
  "name": "whitespace",
  "displayName": "Whitespace",
  "description": "Normalizes whitespace in files",
  "version": "0.0.5",
  "icon": "whitespace.png",
  "publisher": "sandcastle",
  "engines": {
    "vscode": "^0.10.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sandcastle/vscode-whitespace"
  },
  "keywords": [
    "text",
    "whitespace",
    "space",
    "tab",
    "editorconfig"
  ],
  "homepage": "https://github.com/sandcastle/vscode-whitespace",
  "bugs": {
    "url": "https://github.com/sandcastle/vscode-whitespace/issues"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [{
      "command": "extension.whitespace",
      "title": "Normalize Whitespace"
    }],
    "keybindings": [{
      "command": "extension.whitespace",
      "key": "shift+alt+space",
      "mac": "shift+cmd+space",
      "when": "editorTextFocus"
    }]
  },
  "scripts": {
    "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
    "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
  },
  "devDependencies": {
    "typescript": "^1.6.2",
    "vscode": "0.10.x"
  }
}
